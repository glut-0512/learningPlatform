
/*----------------ability_test start------------------*/
.details_content
{
	position: relative;
	width: 1200px;
	margin: 0px auto;
	padding-bottom: 50px;
	height: 100%;
	background-color: #FFFFFF;
	z-index: 5;
}

.details_left
{
	width: 210px;
	float: left;
	margin-top: 10px;
	margin-left: 10px;
}

.dl_content
{
	margin-bottom: 10px;
}

.dl_content_top
{
	height: 60px;
	background-color: #f0f6fa;
	position: relative;
}

.dl_content_top i
{
	float: left;
	margin: 17px 0px 0px 15px;
	font-size:26px ;
	color: #333333;
}

.dl_content_top h1
{
	color: #000000;
	float: left;
	margin: 0px 0px 0px 15px;
	font-size: 16px;
	line-height: 60px;
}

.dl_content ul
{
	margin: 0px;
	background-color: #bacfe3;
}

.dl_content ul li
{
	height: 50px;
}

.checkbox_course
{
	margin: 13px 0px 0px 18px;
	float: left;
	cursor: pointer;
}

.dl_content ul li p
{
	color: #000000;
	font-size: 16px;
	margin: 0px 0px 0px 10px;
	line-height: 50px;
	float: left;
}

.professional{
	width: 950px;
	float: left;
	margin-left: 20px;
}

.search_b1
{
	float: right;
	position: relative;
	width: 270px;
	margin: 10px 10px 10px 0px;
}

.search_b1 input {
	box-sizing: border-box;
	width: 100%;
	height: 30px;
	padding-left: 10px;
	border: 2px solid #beccdb;
	border-radius: 5px;
	outline: none;
	background: #ffffff;
	color: #9E9C9C;
}
.search_b1 button {
	position: absolute; 
	top: 4px;
	right: 4px;
	width: 24px;
	height: 24px;
	border: none;
	outline: none;
	background: url(../img/magnifier.jpg) no-repeat;
	cursor: pointer;
}

.professional .title{
	width: 122px;
	height: 28px;
	margin-bottom: 30px;
	background: url(../img/professional_img_01.png) no-repeat;
} 

.professional .title h1{
	margin: 0px 0px 0px 40px;
	font-size: 16px;
	font-weight: bold;
	color: #FFFFFF;
	line-height: 28px;
}

.professional dl dd{
	width: 110px;
	float: left;
	text-align: center;
	margin: 0px 20px 20px;
}

.professional dl dd img{
	width: 53px;
	height: 56px;
}

.professional dl dd p{
	margin: 0px;
}

.professional dl dd a{
	color: #000000;
	text-decoration: none;
}

.professional dl dd a:hover{
	color: #4a80bd;
}